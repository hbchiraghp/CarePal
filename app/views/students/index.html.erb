<h1 align="center">Listing Students</h1>
<table cellspacing="6" align="center">
   <tr>
    <th>Student Number</th>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Profile pic</th>
    <th>Email</th>
    <th>Gender</th>
    <th>Dob</th>
    <th>Address_1</th>
    <th>Address_2</th>
    <th>city</th>
    <th>Country</th>
    <th>Postcode</th>
    <th>Status</th>
    <th>Deleted</th>

    <th colspan="3">&nbsp;&nbsp;&nbsp;<%= link_to 'Add New user', new_student_path %></th>
    
  </tr>
 
  <% @students.each do |student| %>
    <tr>
    	<td><%= student.student_number %></td>
    	<td><%= student.username %></td>
      <td><%= student.first_name %></td>
      <td><%= student.last_name %></td>
      <td><%= student.profile_picture %></td>
      <td><%= student.email %></td>
      <td><%= student.gender %></td>
      <td><%= student.dob %></td>
      <td><%= student.address_1 %></td>
      <td><%= student.address_2 %></td>
      <td><%= student.city %></td>
      <td><%= student.country %></td>
      <td><%= student.postcode %></td>
      <td><%= student.status %></td>
      <td><%= student.deleted %></td>
      <td><%= link_to 'Show', student_path(student) %></td>
      <td><%= link_to 'Edit', edit_student_path(student) %></td>
      <td><%= link_to 'Delete', student_path(student),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>

</table>
</body>



  
		
		